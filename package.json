{
  "name": "test-app",
  "version": "1.4.1",
  "description": "This API will allow us to operate on app.",
  "main": "index.ts",
  "scripts": {
    "lint": "eslint '*/**/*.{js,ts}' --fix",
    "lintWindows": "eslint \"*/**/*.{js,ts}\" --fix",
    "tsc": "tsc",
    "lintNoFix": "eslint '*/**/*.{js,ts}' --max-warnings=0",
    "test": "jest",
    "test:fast": "cross-env KEEP_DATABASE=true npm run test",
    "pre:pr": "npm run tsc && npm run test && npm run lint",
    "build": "webpack --config webpack.config.js",
    "start": "npm run build && nodemon --config nodemon.json -r dotenv/config  .webpack/api.js",
    "typeorm": "npm run export:env -- ./node_modules/typeorm/cli.js",
    "db:migrate:local": "npm run typeorm migration:run",
    "db:migrate:down": "npm run typeorm migration:revert",
    "db:delete:local": "docker rm -f -v main-db",
    "db:delete:test": "docker rm -f -v main-test-db",
    "db:create": "npm run export:env scripts/database/create.ts",
    "db:new": "npm run db:create && npm run db:migrate:local",
    "db:seed": "npm run export:env scripts/database/seed.ts",
    "db:new:seed": "npm run db:new && npm run db:seed",
    "db:new:generate-migration": "npm run db:new && npm run db:generate-migration",
    "db:generate-migration": "npm run typeorm -- migration:generate -n",
    "export:env": "ts-node -r tsconfig-paths/register -r dotenv/config",
    "start:dev": "nodemon --config nodemon.json -r dotenv/config  src/index.ts",
    "sentry:release": "./scripts/devops/sentry-release.sh $npm_config_stage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@sentry/integrations": "^7.30.0",
    "@sentry/node": "^7.30.0",
    "@sentry/tracing": "^7.30.0",
    "@sentry/types": "^7.30.0",
    "aws-sdk": "^2.1226.0",
    "babel-loader": "^8.2.5",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^2.29.3",
    "express": "^4.17.3",
    "express-fileupload": "^1.4.0",
    "express-validator": "^6.14.0",
    "helmet": "^6.0.0",
    "jest-mock-extended": "^2.0.6",
    "jsonwebtoken": "^8.5.1",
    "nodemon": "^2.0.19",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^10.9.1",
    "typeorm": "^0.2.41",
    "typeorm-transactional-cls-hooked": "^0.1.21",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.4.0",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@sentry/cli": "^2.6.0",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-fileupload": "^1.4.1",
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^15.6.1",
    "@types/pg": "^8.6.5",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "conventional-changelog-conventionalcommits": "^5.0.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.4.6",
    "eslint-plugin-no-async-foreach": "^0.1.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "jest": "^27.5.1",
    "jest-extended": "^3.1.0",
    "jest-mock-extended": "^2.0.9",
    "prettier": "^2.2.1",
    "semantic-release": "^19.0.5",
    "semantic-release-slack-bot": "^3.5.3",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.4",
    "ts-loader": "^9.2.8",
    "tsconfig-paths": "^4.1.2",
    "typeorm-factory": "0.0.14",
    "typeorm-test-transactions": "^3.6.1",
    "typescript": "^4.6.3",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.10.0",
    "webpack-node-externals": "^3.0.0"
  }
}
